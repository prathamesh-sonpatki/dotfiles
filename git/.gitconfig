[user]
    email = csonpatki@gmail.com
    name = Prathamesh Sonpatki
[color]
    status = auto
    diff = auto
    branch = auto
    interactive = auto
    ui = auto
    pager = true

[color "branch"]
    current = blue bold
    local = yellow bold
    remote = green bold

[color "diff"]
    meta = yellow bold
    frag = green bold
    old = red bold
    new = blue bold

[color "status"]
    added = yellow bold
    changed = green bold
    untracked = cyan bold

[core]
    pager = less -FRSX
    whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
	excludesfile = /home/cha1tanya/.gitignore_global

[alias]
  co = checkout
  c = commit -m
  a = add
  aa = !git add -u && git add . && git status
  cob = checkout -b
  up = !git fetch origin && git rebase origin/master
  ir = !git rebase -i origin/master
  done = !git fetch && git rebase origin/master && git checkout master && git merge @{-1} && rake && git push
  count = !git shortlog -s -n
  rup = !git remote update --prune
  gpr = !git pull --rebase
  msg = commit --allow-empty -m
  lg1 = log --graph --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold white)— %an%C(reset)%C(bold yellow)%d%C(reset)' --abbrev-commit --date=relative
  lg2 = log --graph --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(bold white)— %an%C(reset)' --abbrev-commit
  lg = !"git lg1"
  pru = pull --rebase upstream master
  pro = pull --rebase origin master
  cr  = clone --recursive
  ribbon = tag --force _ribbon upstream/master
  catchup = log --patch --reverse --topo-order _ribbon..upstream/master

[help]
  autocorrect = 1
[github]
	user = prathamesh-sonpatki
[push]
	default = matching
